{
  "name": "symon",
  "version": "0.0.1",
  "description": "Synthetic Monitoring",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "start": "concurrently --kill-others -p name -n 'server,client' -c 'blue,green' 'npm run server' 'npm run client'",
    "client": "webpack serve --host 0.0.0.0 --progress --profile --watch --content-base src/ ",
    "server": "npm run server:build && node build/server/index.js",
    "server:build": "tsc",
    "client:build": "cross-env NODE_ENV=production webpack",
    "test": "npm run static-analysis && npm run dynamic-analysis",
    "static-analysis": "npm run check-format && npm run lint",
    "check-format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "format-code": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint-fix": "eslint src --ext js,jsx,ts,tsx --fix",
    "dynamic-analysis": "npm run test:client && npm run test:server",
    "test:client": "jest --config src/client/jest.config.js --coverage",
    "test:server": "jest --config src/server/jest.config.js --coverage",
    "test:server:watch": "jest --config src/server/jest.config.js --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:open": "cypress open",
    "migrate": "prisma migrate dev && prisma generate",
    "seed": "cd src/server && prisma db seed --preview-feature && cd ../../../"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperjumptech/symon.git"
  },
  "bugs": {
    "url": "https://github.com/hyperjumptech/symon/issues"
  },
  "homepage": "https://github.com/hyperjumptech/symon#readme",
  "dependencies": {
    "@hyperjumptech/monika": "^1.3.1",
    "@prisma/client": "2.23.0",
    "argon2": "^0.27.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "date-fns": "2.21.2",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-winston": "^4.0.5",
    "joi": "17.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-query": "^3.8.2",
    "react-router-dom": "^5.2.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@tailwindcss/forms": "^0.2.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-winston": "^4.0.0",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/passport": "^1.0.5",
    "@types/passport-jwt": "^3.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^10.2.3",
    "concurrently": "~5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cypress": "^6.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.2.0",
    "html-webpack-plugin": "^5.0.0-beta.5",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "^1.3.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "prettier": "~2.2.1",
    "prisma": "^2.23.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "~8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.3",
    "webpack": "~5.15.0",
    "webpack-cli": "~4.3.1",
    "webpack-dev-server": "~3.11.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "prisma": {
    "schema": "src/server/prisma/schema.prisma"
  }
}
